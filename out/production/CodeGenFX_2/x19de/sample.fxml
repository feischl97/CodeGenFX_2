<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<VBox fx:controller="x19de.Controller"
      xmlns:fx="http://javafx.com/fxml" alignment="center">

    <BorderPane.margin>
        <Insets bottom="2.50" left="2.50" right="2.50" top="2.50" />
    </BorderPane.margin>

    <BorderPane fx:id="ctrls" maxWidth="300"
                style=" -fx-border-color:black; -fx-border-width: 1; -fx-border-style: solid;">



        <top>
            <ComboBox fx:id="barcodeTypes"/>
        </top>
        <center>
            <Pane fx:id="barcodeSettings" BorderPane.alignment="CENTER"
                  prefHeight="200"
                  prefWidth="200" maxWidth="300"
                  style=" -fx-border-color:black; -fx-border-width: 1; -fx-border-style: solid;">
            </Pane>
        </center>
        <bottom>
            <HBox fx:id="generalCtrl" BorderPane.alignment="BOTTOM_CENTER" minHeight="75">
                <HBox.margin>
                    <Insets bottom="2.50" left="2.50" right="2.50" top="2.50" />
                </HBox.margin>

                <Button fx:id="exit" text="Exit">
                    <Insets bottom="2.50" left="2.50" right="2.50" top="2.50" />
                </Button>
                <Button fx:id="export" text="Save As">
                    <Insets bottom="2.50" left="2.50" right="2.50" top="2.50" />
                </Button>
                <Button fx:id="generate" text="Generate">
                    <Insets bottom="2.50" left="2.50" right="2.50" top="2.50" />
                </Button>
            </HBox>
        </bottom>
    </BorderPane>

    <ImageView fx:id="previewBox"

               style=" -fx-border-color:black; -fx-border-width: 1; -fx-border-style: solid;"/>
</VBox>